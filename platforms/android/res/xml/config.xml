<?xml version='1.0' encoding='utf-8'?>
<widget id="id.cepatsembuh" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <icon density="mdpi" src="res/mipmap-mdpi/ic_launcher.png" />
    <icon density="hdpi" src="res/mipmap-hdpi/ic_launcher.png" />
    <icon density="xhdpi" src="res/mipmap-xhdpi/ic_launcher.png" />
    <icon density="xxhdpi" src="res/mipmap-xxhdpi/ic_launcher.png" />
    <icon density="xxxhdpi" src="res/mipmap-xxxhdpi/ic_launcher.png" />
    <splash density="port-mdpi" src="res/splash.png" />
    <splash density="port-hdpi" src="res/splash.png" />
    <splash density="port-xhdpi" src="res/splash.png" />
    <name>CepatSembuh</name>
    <description>
      An app to help people to find the nearest faskes in their location
    </description>
    <author>
      Andre Christoga
    </author>
    <content src="www/index.html" />
    <access origin="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="https://cepatsembuh.firebaseio.com" />
    <preference name="Orientation" value="portrait" />
</widget>
